// Online C compiler to run C program online
#include <stdio.h>

int main() {
  int i,j,k;
  for(i=0;i<=10;i++)
  {
      for(j=1;j<=10-i;j++)
      {
          printf(" ");
      }
  
      for(k=i;k>=1;k--)
      {
          printf("%d",k );
      }
      for(k=1;k<=i;k++)
      {
          printf("%d",k);
      }
      printf("\n");
  }

    return 0;
}
