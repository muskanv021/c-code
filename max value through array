// Online C compiler to run C program online
#include <stdio.h>
#define size 10

int main() {
    int a[size],i,max,mxp=0;
    printf("Enter %d value",size);
    for(i=0;i<size;i++)
    {
        scanf("%d",&a[i]);
    }
    max=a[0];
    for(i=1;i<size;i++)
    {
        if(a[i]>max){
            max=a[i];
            mxp=i;
        }
    }
    printf("\n Max of all the values:%d",max);
    printf("\n position Max of values:%d",mxp);

    return 0;
}
